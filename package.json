{
  "name": "express-mongoose-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "server": "node --inspect ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "build": "npm-run-all clean transpile",
    "launch": "node ./server/bin/www",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "format": "prettier --write \"server/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.11.15",
    "mongoose-cursor-pagination": "^1.0.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ulid": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.15",
    "husky": "^5.0.9",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false,
          "version": "7.0.0-beta.0"
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
